{
    "name": "vs-spero",
    "version": "1.0.0",
    "description": "Spero support for Visual Studio Code",
    "engines": {
        "vscode": ">=0.9.0-pre.1"
    },
    "publisherName": "hGriff0n",
    "publisher": "graysonhooper",
    "categories": [
        "Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/hGriff0n/vs-spero.git"
    },
    "contributes": {
        "languages": [
            {
                "id": "Spero",
                "extensions": [
                    ".spr",
                    ".spqr"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "Spero",
                "scopeName": "source.spero",
                "path": "./syntaxes/spero.tmLanguage"
            }
        ]
    },
    "editor.tabSize": 4
}